<div class="pl-4 text-xs max-h-screen overflow-auto top-0 bg-white lg:w-52 max-xl:hidden">
	{# Menu 1 #}
	<div class="pt-2 pb-2 mb-1 bg-white whitespace-nowrap w-full">
		<div class="flex flex-col">
			{% for menu_item in menu %} {% set active_style = (selected_menu_index is
			defined and loop.index0 == selected_menu_index) ? 'bg-gray-300' :
			'bg-white' %}
			<a
				href="?menu_index={{ loop.index0 }}"
				class="flex items-center  px-3 py-2 rounded-md transition hover:bg-gray-200 {{
					active_style
				}}">
				{{ menu_item.icon | raw }}
				<span class="pl-2">{{ menu_item.title }}</span>
			</a>
			{% endfor %}
		</div>
	</div>
	<hr class="my-1" />

	{# Menu 2 #}

	<div class="pt-2 pb-2 mb-1 bg-white whitespace-nowrap w-full">
		<div class="flex flex-col">
			{% for menu2_item in menu2 %} {% set active_style2 = (selected_menu2_index
			is defined and loop.index0 == selected_menu2_index) ? 'bg-gray-300' :
			'bg-white' %}
			<a
				href="?menu2_index={{ loop.index0 }}"
				class="flex items-center  px-3 py-2 rounded-md transition hover:bg-gray-200 {{
					active_style2
				}}">
				{{ menu2_item.icon | raw }}
				<span class="pl-2">{{ menu2_item.title }}</span>
			</a>
			{% endfor %}
		</div>
	</div>
	<hr class="my-1" />

	{# Menu 3 #}

	<div class="pt-2 pb-2 mb-1 bg-white whitespace-nowrap w-full">
		<div class="flex flex-col">
			{% for menu3_item in menu3 %} {% set active_style3 = (selected_menu3_index
			is defined and loop.index0 == selected_menu3_index) ? 'bg-gray-300' :
			'bg-white' %}
			<a
				href="?menu3_index={{ loop.index0 }}"
				class="flex items-center  px-3 py-2 rounded-md transition hover:bg-gray-200 {{
					active_style3
				}}">
				{{ menu3_item.icon | raw }}
				<span class="pl-2">{{ menu3_item.title }}</span>
			</a>
			{% endfor %}
		</div>
	</div>
	{# Expand #}
	<div class="relative">
		<button
			type="button"
			class="w-full flex items-center justify-start items-center p-1 bg-white text-gray-700 hover:bg-gray-100"
			id="menu-button"
			aria-expanded="false"
			aria-haspopup="true">
			<svg
				xmlns="http://www.w3.org/2000/svg"
				fill="none"
				viewBox="0 0 24 24"
				stroke-width="1.5"
				stroke="currentColor"
				class="w-4 h-5">
				<path
					stroke-linecap="round"
					stroke-linejoin="round"
					d="M19.5 8.25l-7.5 7.5-7.5-7.5" />
			</svg>
			<p class="text-gray-700 mx-3">Expand</p>
		</button>

		<div
			class="origin-top-right absolute right-0 mt-2 w-full bg-white ring-1 ring-black ring-opacity-5 hidden"
			role="menu"
			aria-orientation="vertical"
			aria-labelledby="menu-button"
			tabindex="-1">
			<div class="py-1" role="none">
				<div class="pt-2 pb-2 mb-1 bg-white whitespace-nowrap w-full">
					<div class="flex flex-col">
						{% for expand_item in expand %} {% set active_style4 =
						(selected_expand_index is defined and loop.index0 ==
						selected_expand_index) ? 'bg-gray-300' : 'bg-white' %}
						<a
							href="?expand_index={{ loop.index0 }}"
							class="flex items-center  px-3 py-2 rounded-sm transition hover:bg-gray-200 {{
								active_style3
							}}">
							{{ expand_item.icon | raw }}
							<span class="pl-2">{{ expand_item.title }}</span>
						</a>
						{% endfor %}
					</div>
				</div>
			</div>
		</div>
	</div>
	<hr class="my-1" />
	{# Footer #}
	<footer class="text-gray-600">
		<div class="p-4">
			{% for link in links_footer %}

			<a href="{{ link.url }}" class="">{{ link.title }}</a>

			{% endfor %}
		</div>
		{# Footer 2 #}
		<div class="p-4">
			{% for link in links_footer2 %}

			<a href="{{ link.url }}" class="">{{ link.title }}</a>

			{% endfor %}
		</div>
		<div class="pl-4 text-xs text-gray-500">
			Â© 2023 Google LLC
			<div />
		</div>
	</footer>
</div>





{#sidebar_mini#}

	<div class="pt-2 pb-2 mb-1 bg-white whitespace-nowrap w-full xl:hidden max-lg:hidden">
		<div class="flex flex-col">
			{% for menu_item in menu %} {% set active_style = (selected_menu_index is
			defined and loop.index0 == selected_menu_index) ? 'bg-gray-300' :
			'bg-white' %}
			<a
				href="?menu_index={{ loop.index0 }}"
				class="flex items-center  px-3 py-2 rounded-sm transition hover:bg-gray-200 {{
					active_style
				}}">
				{{ menu_item.icon | raw }}
				<span class="pl-2">{{ menu_item.title }}</span>
			</a>
			{% endfor %}
		</div>
	</div>







<script>
	const menuButton = document.getElementById("menu-button")
	const menuItems = document.querySelectorAll('[role="menuitem"]')

	menuButton.addEventListener("click", () => {
		const expanded =
			menuButton.getAttribute("aria-expanded") === "true" || false
		menuButton.setAttribute("aria-expanded", !expanded)
		menuButton.nextElementSibling.classList.toggle("hidden")
	})

	menuItems.forEach((item) => {
		item.addEventListener("click", () => {})
	})
</script>
